#!/bin/csh -f

cd `dirname $0` && source ./Configuration

#
# Get the process ID of the kibana instance if it is running.
#
set KI_PID=`ps -aef | grep "mgiadmin" | grep "kibana" | grep -v grep | awk '{print $2}'`

#
# If kibana is running, kill it.
#
if ( "${KI_PID}" != "" ) then
    echo "Kill KI process: ${KI_PID}"
    kill -9 ${KI_PID}
else
    echo "No kibana running."
endif

